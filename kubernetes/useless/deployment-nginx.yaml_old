apiVersion: apps/v1
kind: Deployment
metadata:
	name: nginx-deployment
spec:
	selector:
		matchLabels:
			app: nginx
	replicas: 1
	template:
		metadata:
			labels:
				app: nginx
		spec:
			containers:
				- name: nginx
					image: ishank162/nginx-m:v11
					ports:
						- containerPort: 8080

---
apiVersion: v1
kind: Service
metadata:
	name: nginx-service
spec:
	selector:
		app: nginx
	ports:
		- port: 8080
			targetPort: 8080

